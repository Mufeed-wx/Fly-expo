<div class="wishlist_body" id="whishlist">
	<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/boxicons@2.0.7/css/boxicons.min.css">



	<div class="row row-cols-1 row-cols-lg-4 row-cols-xl-4">
       {{#if wishlist.Wishlistdata}}
		{{#each wishlist.Wishlistdata}}
		<div class="col">
			<div class="carda radius-15">
				<div class="card-body text-center">
					<div class="p-1 border radius-15">
						<img src="/{{this.productId.image.[0]}}" width="150" height="150" class=" " alt="">
						<h5 class="mb-0 ">{{this.productId.Product_Name}}</h5>
						<p class="mb-3">Price: ${{this.productId.Product_Prize}}/kg</p>
						<div class="list-inline contacts-social mt-3 mb-3">
							<button href="" class="list-inline-item bg-shopping text-white border-0"><i
									class="fas fa-shopping-cart"></i></button>
							<button data-id="{{this.productId._id}}" href="" id="delete-wishlist"
								onclick="deleteWhishlist(this);" class="list-inline-item bg-heart text-white border-0"><i
									class="fas fa-window-close fa-spin "></i></i></button>
							<button href="" class="list-inline-item bg-full text-white border-0"><i
									class="fas fa-expand"></i></button>
						</div>
					</div>
				</div>
			</div>
		</div>
		{{/each}}
		{{else}}
		<h1>No data</h1>
		{{/if}}

	</div>

</div>
